<?xml version="1.0" encoding="UTF-8"?>
<businessContext prefix="auth" name="userLoggedIn">
    <includedContexts />
    <preconditions>
        <precondition name="User must be signed in">
            <condition>
                <isNotNull>
                    <argument>
                        <variableReference name="user" type="object" />
                    </argument>
                </isNotNull>
            </condition>
        </precondition>
    </preconditions>
    <postConditions />
</businessContext>
