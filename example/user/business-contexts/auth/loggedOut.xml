<?xml version="1.0" encoding="UTF-8"?>
<businessContext prefix="auth" name="loggedOut">
    <includedContexts/>
    <preconditions>
        <precondition name="User must not be logged in">
            <condition>
                <logicalNegate>
                    <argument>
                        <isNotNull>
                            <argument>
                                <variableReference name="user" type="boolean"/>
                            </argument>
                        </isNotNull>
                    </argument>
                </logicalNegate>
            </condition>
        </precondition>
    </preconditions>
    <postConditions/>
</businessContext>
